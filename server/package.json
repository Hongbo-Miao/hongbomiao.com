{
  "name": "server",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc && babel build --out-dir build",
    "dev": "nodemon --ext ts --exec \"yarn build && NODE_ENV=development node --require dotenv-flow/config build/index.js\"",
    "serve": "NODE_ENV=production node --require dotenv-flow/config build/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git"
  },
  "dependencies": {
    "@godaddy/terminus": "4.4.1",
    "@opentelemetry/api": "0.10.2",
    "@opentelemetry/exporter-collector": "0.10.2",
    "@opentelemetry/node": "0.10.2",
    "@opentelemetry/plugin-express": "0.9.0",
    "@opentelemetry/plugin-http": "0.10.2",
    "@opentelemetry/plugin-https": "0.10.2",
    "@opentelemetry/tracing": "0.10.2",
    "@sentry/node": "5.20.1",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "express": "5.0.0-alpha.8",
    "express-graphql": "0.11.0",
    "graphql": "15.3.0",
    "helmet": "4.0.0",
    "lightstep-opentelemetry-exporter": "0.2.1",
    "morgan": "1.10.0",
    "redirect-ssl": "2.0.0",
    "spdy": "4.0.2",
    "winston": "3.3.3",
    "winston-transport-sentry-node": "0.6.0"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.4",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-typescript": "7.10.4",
    "@types/cors": "2.8.7",
    "@types/dotenv-flow": "3.0.0",
    "@types/express": "4.17.7",
    "@types/express-rate-limit": "5.1.0",
    "@types/jest": "26.0.10",
    "@types/morgan": "1.9.1",
    "@types/node": "14.6.0",
    "@types/spdy": "3.4.4",
    "@types/supertest": "2.0.10",
    "dotenv-flow": "3.2.0",
    "express-rate-limit": "5.1.3",
    "jest": "26.3.0",
    "nodemon": "2.0.4",
    "supertest": "4.0.2",
    "typescript": "3.9.7"
  }
}
