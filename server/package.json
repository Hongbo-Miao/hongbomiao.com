{
  "name": "server",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=14.11.0"
  },
  "scripts": {
    "build": "tsc && babel build --out-dir build",
    "debug": "yarn build && cross-env NODE_ENV=development node --inspect-brk build/index.js",
    "dev": "nodemon --ext ts --exec 'yarn build && cross-env NODE_ENV=development node build/index.js --hideHTTPLog --prettifyLog'",
    "dev:log": "nodemon --ext ts --exec 'yarn build && cross-env NODE_ENV=development node build/index.js --prettifyLog'",
    "dev:cypress": "nodemon --ext ts --exec 'yarn build && cross-env NODE_ENV=test node build/index.js'",
    "dev:pm2": "nodemon --ext ts --exec 'yarn build && pm2 start ecosystem.config.js --env development'",
    "dev:pm2:stop": "pm2 stop server",
    "dev:pm2:delete": "pm2 delete server",
    "dev:pm2:monit": "pm2 monit",
    "serve:production": "cross-env NODE_ENV=production node build/index.js",
    "serve:pm2:development": "pm2-runtime start ecosystem.config.js --env development",
    "serve:pm2:production": "pm2-runtime start ecosystem.config.js --env production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "autocannon": "autocannon http://localhost:5000",
    "profile:doctor": "cross-env NODE_ENV=production clinic doctor --on-port 'autocannon --connections 5 --amount 500 http://localhost:5000' -- node build/index.js",
    "profile:bubbleprof": "cross-env NODE_ENV=production clinic bubbleprof --on-port 'autocannon --connections 5 --amount 500 http://localhost:5000' -- node build/index.js",
    "profile:flame": "cross-env NODE_ENV=production clinic flame --on-port 'autocannon --connections 5 --amount 500 http://localhost:5000' -- node build/index.js"
  },
  "repository": {
    "type": "git"
  },
  "dependencies": {
    "@godaddy/terminus": "4.4.1",
    "@opentelemetry/api": "0.11.0",
    "@opentelemetry/exporter-collector": "0.11.0",
    "@opentelemetry/metrics": "0.11.0",
    "@opentelemetry/node": "0.11.0",
    "@opentelemetry/plugin-express": "0.10.0",
    "@opentelemetry/plugin-http": "0.11.0",
    "@opentelemetry/plugin-https": "0.11.0",
    "@opentelemetry/plugin-ioredis": "0.10.0",
    "@opentelemetry/plugin-pg": "0.10.0",
    "@opentelemetry/plugin-pg-pool": "0.10.0",
    "@opentelemetry/tracing": "0.11.0",
    "@sentry/node": "5.24.2",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "dataloader": "2.0.0",
    "express": "5.0.0-alpha.8",
    "express-graphql": "0.11.0",
    "express-request-id": "1.4.1",
    "faker": "5.1.0",
    "graphql": "15.3.0",
    "helmet": "4.1.1",
    "ioredis": "4.17.3",
    "knex": "0.21.6",
    "lodash.uniq": "4.5.0",
    "pg": "8.3.3",
    "pino": "6.6.1",
    "pino-http": "5.3.0",
    "rate-limiter-flexible": "2.1.10",
    "response-time": "2.3.2",
    "serve-favicon": "2.5.0",
    "spdy": "4.0.2",
    "validator": "13.1.17",
    "yargs": "16.0.3"
  },
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-typescript": "7.10.4",
    "@types/cors": "2.8.7",
    "@types/dotenv-flow": "3.1.0",
    "@types/express": "4.17.8",
    "@types/express-request-id": "1.4.1",
    "@types/faker": "5.1.2",
    "@types/ioredis": "4.17.4",
    "@types/jest": "26.0.14",
    "@types/lodash.uniq": "4.5.6",
    "@types/node": "14.11.2",
    "@types/pino-http": "5.0.5",
    "@types/response-time": "2.3.4",
    "@types/serve-favicon": "2.5.0",
    "@types/spdy": "3.4.4",
    "@types/supertest": "2.0.10",
    "@types/validator": "13.1.0",
    "@types/yargs": "15.0.7",
    "autocannon": "6.5.0",
    "clinic": "7.0.0",
    "cross-env": "7.0.2",
    "dotenv-flow": "3.2.0",
    "ioredis-mock": "4.21.4",
    "jest": "26.4.2",
    "nodemon": "2.0.4",
    "pino-pretty": "4.2.1",
    "pm2": "4.5.0",
    "supertest": "5.0.0",
    "typescript": "4.0.3"
  }
}
