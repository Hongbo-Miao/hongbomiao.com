{
  "name": "server",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc && babel build --out-dir build",
    "start": "nodemon --ext ts --exec \"yarn build && NODE_ENV=devlopment node --require dotenv/config build/index.js\"",
    "serve": "NODE_ENV=production node --require dotenv/config build/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git"
  },
  "dependencies": {
    "@sentry/node": "5.20.1",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "express": "5.0.0-alpha.8",
    "helmet": "4.0.0",
    "morgan": "1.10.0",
    "redirect-ssl": "2.0.0",
    "spdy": "4.0.2",
    "winston": "3.3.3",
    "winston-transport-sentry-node": "0.6.0"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.0",
    "@babel/preset-env": "7.11.0",
    "@babel/preset-typescript": "7.10.4",
    "@types/cors": "2.8.7",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.7",
    "@types/express-rate-limit": "5.0.0",
    "@types/helmet": "0.0.47",
    "@types/jest": "26.0.8",
    "@types/morgan": "1.9.1",
    "@types/spdy": "3.4.4",
    "dotenv": "8.2.0",
    "express-rate-limit": "5.1.3",
    "jest": "26.2.2",
    "nodemon": "2.0.4"
  }
}
