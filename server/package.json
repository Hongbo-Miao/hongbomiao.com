    "@types/pino-http": "5.0.5",
    "@types/response-time": "2.3.4",
    "@types/serve-favicon": "2.5.1",
    "@types/spdy": "3.4.4",
    "@types/supertest": "2.0.10",
    "@types/validator": "13.1.0",
    "@types/yargs": "15.0.9",
    "cross-env": "7.0.2",
    "dotenv-flow": "3.2.0",
    "ioredis-mock": "5.1.0",
    "jest": "26.6.3",
    "nodemon": "2.0.6",
    "pino-pretty": "4.3.0",
    "pm2": "4.5.0",
    "supertest": "6.0.1",
    "typescript": "4.0.5"
  },
  "optionalDependencies": {
    "autocannon": "6.5.0",
    "clinic": "7.0.0",
    "express-list-endpoints": "5.0.0"
  }
}
