{
  "name": "server",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": "15.x"
  },
  "scripts": {
    "build": "tsc && babel build --out-dir build",
    "debug": "yarn build && cross-env NODE_ENV=development node --inspect-brk build/src/index.js",
    "dev": "nodemon --ext ts --exec 'yarn build && cross-env NODE_ENV=development node build/src/index.js --hideHTTPLog --prettifyLog'",
    "dev:log": "nodemon --ext ts --exec 'yarn build && cross-env NODE_ENV=development node build/src/index.js --prettifyLog'",
    "dev:cypress": "nodemon --ext ts --exec 'yarn build && cross-env NODE_ENV=test node build/src/index.js'",
    "dev:pm2": "nodemon --ext ts --exec 'yarn build && pm2 start ecosystem.config.js --env development'",
    "dev:pm2:stop": "pm2 stop server",
    "dev:pm2:delete": "pm2 delete server",
    "dev:pm2:monit": "pm2 monit",
    "serve:development": "cross-env NODE_ENV=development node build/src/index.js",
    "serve:production": "cross-env NODE_ENV=production node build/src/index.js",
    "serve:pm2:development": "pm2-runtime start ecosystem.config.js --env development",
    "serve:pm2:production": "pm2-runtime start ecosystem.config.js --env production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "autocannon": "autocannon http://localhost:5000",
    "profile:endpoints": "yarn build && NODE_ENV=production node build/scripts/profileEndpoints.js",
    "profile:bubbleprof": "yarn build && cross-env NODE_ENV=production clinic bubbleprof --on-port 'sh bin/profile.sh' -- node build/src/index.js",
    "profile:doctor": "yarn build && cross-env NODE_ENV=production clinic doctor --on-port 'sh bin/profile.sh' -- node build/src/index.js",
    "profile:flame": "yarn build && cross-env NODE_ENV=production clinic flame --on-port 'sh bin/profile.sh' -- node build/src/index.js"
  },
  "repository": {
    "type": "git"
  },
  "dependencies": {
    "@godaddy/terminus": "4.4.1",
    "@opentelemetry/api": "0.12.0",
    "@opentelemetry/exporter-collector": "0.12.0",
    "@opentelemetry/metrics": "0.12.0",
    "@opentelemetry/node": "0.12.0",
    "@opentelemetry/plugin-dns": "0.10.0",
    "@opentelemetry/plugin-express": "0.10.0",
    "@opentelemetry/plugin-http": "0.12.0",
    "@opentelemetry/plugin-https": "0.12.0",
    "@opentelemetry/plugin-ioredis": "0.10.0",
    "@opentelemetry/plugin-pg": "0.10.0",
    "@opentelemetry/plugin-pg-pool": "0.10.0",
    "@opentelemetry/tracing": "0.12.0",
    "@sentry/node": "5.27.3",
    "axios": "0.21.0",
    "body-parser": "1.19.0",
    "connect-timeout": "1.9.0",
    "cors": "2.8.5",
    "dataloader": "2.0.0",
    "express": "5.0.0-alpha.8",
    "express-graphql": "0.11.0",
    "express-request-id": "1.4.1",
    "faker": "5.1.0",
    "graphql": "15.4.0",
    "graphql-subscriptions": "1.1.0",
    "graphql-ws": "1.11.0",
    "helmet": "4.2.0",
    "ioredis": "4.19.2",
    "knex": "0.21.12",
    "lodash.uniq": "4.5.0",
    "network-error-logging": "1.2.1",
    "opossum": "git://github.com/nodeshift/opossum#c5ca2187f137f11aeeeffe1fe83caf3d785fbbb4",
    "pg": "8.4.2",
    "pino": "6.7.0",
    "pino-http": "5.3.0",
    "rate-limiter-flexible": "2.1.13",
    "report-to": "1.1.0",
    "response-time": "2.3.2",
    "serve-favicon": "2.5.0",
    "spdy": "4.0.2",
    "validator": "13.1.17",
    "yargs": "16.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-typescript": "7.12.1",
    "@types/autocannon": "4.1.0",
    "@types/connect-timeout": "0.0.34",
    "@types/cors": "2.8.8",
    "@types/dotenv-flow": "3.1.0",
    "@types/express": "4.17.8",
    "@types/express-list-endpoints": "4.0.1",
    "@types/express-request-id": "1.4.1",
    "@types/faker": "5.1.4",
    "@types/ioredis": "4.17.7",
    "@types/jest": "26.0.15",
