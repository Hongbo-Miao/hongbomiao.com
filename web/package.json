{
  "name": "web",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "dev": "react-scripts start",
    "prebuild": "sh ./bin/prebuild.sh",
    "build": "react-scripts build",
    "postbuild": "sh ./bin/postbuild.sh",
    "serve": "serve build --listen 8080",
    "analyze": "react-scripts build && source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test --watchAll=false",
    "test:watch": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "storybook": "start-storybook --static-dir public --port 9009",
    "storybook:build": "build-storybook -s public",
    "storybook:publish": "chromatic --build-script-name storybook:build",
    "lighthouse": "lhci autorun",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      ".query.ts",
      ".story.tsx",
      ".type.ts",
      "src/shared/libs/*",
      "src/shared/utils/initSentry.ts"
    ]
  },
  "dependencies": {
    "@analytics/google-tag-manager": "0.5.0",
    "@opentelemetry/api": "0.18.1",
    "@opentelemetry/exporter-collector": "0.18.2",
    "@opentelemetry/instrumentation": "0.18.2",
    "@opentelemetry/instrumentation-document-load": "0.14.0",
    "@opentelemetry/instrumentation-xml-http-request": "0.18.2",
    "@opentelemetry/tracing": "0.18.2",
    "@opentelemetry/web": "0.18.2",
    "@sentry/react": "6.2.5",
    "analytics": "0.7.5",
    "bulma": "0.9.2",
    "classnames": "2.3.0",
    "graphql": "15.5.0",
    "graphql-tag": "2.11.0",
    "graphql-ws": "4.3.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.3",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "react-use": "17.2.1",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.9",
    "redux-observable": "1.2.0",
    "rxjs": "6.6.7",
    "web-vitals": "1.1.1",
    "workbox-background-sync": "6.1.2",
    "workbox-broadcast-update": "6.1.2",
    "workbox-cacheable-response": "6.1.2",
    "workbox-core": "6.1.2",
    "workbox-expiration": "6.1.2",
    "workbox-google-analytics": "6.1.2",
    "workbox-navigation-preload": "6.1.2",
    "workbox-precaching": "6.1.2",
    "workbox-range-requests": "6.1.2",
    "workbox-routing": "6.1.2",
    "workbox-strategies": "6.1.2",
    "workbox-streams": "6.1.2"
  },
  "devDependencies": {
    "@babel/cli": "7.13.14",
    "@babel/plugin-transform-react-display-name": "7.12.13",
    "@babel/preset-env": "7.13.12",
    "@babel/preset-react": "7.13.13",
    "@babel/preset-typescript": "7.13.0",
    "@lhci/cli": "0.7.0",
    "@storybook/addon-actions": "6.2.1",
    "@storybook/addon-links": "6.2.1",
    "@storybook/addons": "6.2.1",
    "@storybook/preset-create-react-app": "3.1.7",
    "@storybook/react": "6.2.1",
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "11.2.6",
    "@testing-library/react-hooks": "5.1.1",
    "@testing-library/user-event": "13.1.1",
    "@types/classnames": "2.2.11",
    "@types/jest": "26.0.22",
    "@types/node": "14.14.37",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-redux": "7.1.16",
    "@types/react-router-dom": "5.1.7",
    "@types/redux-mock-store": "1.0.2",
    "@types/storybook__react": "5.2.1",
    "chromatic": "5.7.1",
    "purgecss": "4.0.3",
    "react-test-renderer": "17.0.2",
    "redux-mock-store": "1.5.4",
    "serve": "11.3.2",
    "sitemap": "6.4.0",
    "source-map-explorer": "2.5.2",
    "typescript": "4.2.3"
  }
}
