{
  "name": "api",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "build": "tsc && babel build --out-dir build",
    "debug": "yarn build && cross-env NODE_ENV=development node --inspect-brk build/src/index.js",
    "dev": "nodemon --ext ts --exec 'yarn build && cross-env NODE_ENV=development node build/src/index.js --hideHTTPLog --prettifyLog'",
    "dev:log": "nodemon --ext ts --exec 'yarn build && cross-env NODE_ENV=development node build/src/index.js --prettifyLog'",
    "dev:pm2": "nodemon --ext ts --exec 'yarn build && pm2 start ecosystem.config.js --env development'",
    "dev:pm2:stop": "pm2 stop api",
    "dev:pm2:delete": "pm2 delete api",
    "dev:pm2:monit": "pm2 monit",
    "serve:development": "cross-env NODE_ENV=development node build/src/index.js",
    "serve:production": "cross-env NODE_ENV=production node build/src/index.js",
    "serve:pm2:development": "pm2-runtime start ecosystem.config.js --env development",
    "serve:pm2:production": "pm2-runtime start ecosystem.config.js --env production",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "autocannon": "autocannon http://localhost:5000",
    "profile:endpoints": "yarn build && NODE_ENV=production node build/scripts/profileEndpoints.js",
    "profile:bubbleprof": "yarn build && cross-env NODE_ENV=production clinic bubbleprof --on-port 'sh bin/profile.sh' -- node build/src/index.js",
    "profile:doctor": "yarn build && cross-env NODE_ENV=production clinic doctor --on-port 'sh bin/profile.sh' -- node build/src/index.js",
    "profile:flame": "yarn build && cross-env NODE_ENV=production clinic flame --on-port 'sh bin/profile.sh' -- node build/src/index.js"
  },
  "dependencies": {
    "@godaddy/terminus": "4.6.0",
    "@opentelemetry/api": "0.16.0",
